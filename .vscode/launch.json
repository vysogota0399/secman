    {
      "name": "Enable Engine",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/cmd/cli/main.go",
      "env": {
        "SERVER_URL": "https://127.0.0.1:8443",
        "LOG_LEVEL": "-1",
        "ROOT_TOKEN": "l3TCi91S2SnFIzbXuWOJZhOk4EsF8MM2HYJ9sLWIPkI=",
        "SSL_SKIP_VERIFY": "true"
      },
      "args": [
        "engine",
        "enable",
        "auth/logopass",
        "token_ttl=1000000",
        "secret_key=1234567890"
      ],
      "showLog": true
    },
    {
      "name": "Attach to Process",
      "type": "go",
      "request": "attach",
      "mode": "local",
      "processId": 1734
